<# 1 #>
function q1($var1,$var2,$var3,$var4) {
    <# Return the product of the arguments #>
    $thing1 = $var1 *= $var2
    $thing2 = $thing1 *= $var3
    $product = $thing2 *= $var4
    echo $product
}
