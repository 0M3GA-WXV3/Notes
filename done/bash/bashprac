function q3 {
    <# In a loop, prompt the user to enter positive integers one at time.
       Stop when the user enters a -1. Return the maximum positive
       value that was entered."
	#>function question3{
    Begin{
        $numbers = @()
        }
        Process{
            do{
                $nums = Read-Host
                $numbers += $nums
        if ($nums -gt -1){
            
        }
            }until($nums -eq -1){
                return ($numbers | Measure-Object -Maximum).Maximum
            }
        }
        End{
        continue
        }
}
question3
}
